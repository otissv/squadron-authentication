{
  "name": "squadron-authentication",
  "version": "1.0.0",
  "description": "GraphQL Authentication Service with Node and Redis ",
  "scripts": {
    "start": "nodemon --exec npm run babel-node -- ./server/index.js",
    "babel-node": "babel-node",
    "lint": "eslint --quiet './' && echo 'Completed:' && date || true && echo 'Completed:'",
    "lint:watch": "watch 'npm run lint' ./",
    "tape": "babel-node test-runner.js | tap-min || true",
    "tape:spec": "babel-node test-runner.js | tap-difflet || true",
    "test": "npm run tape",
    "test:spec": "npm run tape:spec",
    "test:watch": "watch 'npm run tape' ",
    "seed": "npm run babel-node -- ./seed/index.js"
  },
  "author": "Otis Virginie",
  "keywords": [
    "node",
    "express",
    "api",
    "graphql",
    "jwt",
    "json web token",
    "redis",
    "es6"
  ],
  "license": "MIT",
  "dependencies": {
    "apollo-server": "~0.1.2",
    "babel-cli": "~6.14.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "~3.4.6",
    "body-parser": "~1.15.2",
    "compression": "~1.6.1",
    "connect-mongo": "~1.3.2",
    "cookie-parser": "~1.4.1",
    "cors": "~2.8.0",
    "csurf": "~1.9.0",
    "express": "~4.14.0",
    "express-enforces-ssl": "~1.1.0",
    "express-logger": "0.0.3",
    "express-session": "~1.14.0",
    "get-env": "^0.5.1",
    "graphql": "~0.6.2",
    "helmet": "~2.2.0",
    "is-schema-valid": "~0.2.0",
    "isemail": "~2.2.1",
    "jsonwebtoken": "~7.1.9",
    "libphonenumber-node": "0.0.3",
    "map-async-promise": "~2.0.1",
    "method-override": "~2.3.5",
    "nodemon": "~1.10.2",
    "promise-redis": "0.0.5",
    "ramda": "~0.22.1",
    "redis": "~2.6.0-1",
    "rethinkdbdash": "^2.3.23",
    "ufunc": "~2.0.0"
  },
  "devDependencies": {
    "axios": "~0.14.0",
    "babel-eslint": "~6.1.2",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-stage-2": "~6.13.0",
    "eslint": "~3.5.0",
    "eslint-config-standard": "~6.0.0-beta.3",
    "eslint-plugin-graphql": "~0.2.4",
    "eslint-plugin-promise": "~2.0.1",
    "eslint-plugin-standard": "~2.0.0",
    "faucet": "0.0.1",
    "glob": "~7.1.0",
    "morgan": "~1.7.0",
    "tap-difflet": "~0.6.0",
    "tap-min": "~1.1.0",
    "tape": "~4.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
